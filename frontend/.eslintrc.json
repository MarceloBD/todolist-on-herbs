{
    "parser": "babel-eslint",
    "extends": ["airbnb","eslint-config-prettier", "prettier"],    
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": ["react"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "modules": true,
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
    },
    "rules": {
      "func-names": 0,
      "no-param-reassign": 0,
      "no-multi-str": 0,
      "consistent-return": 0,
      "react/jsx-filename-extension": [0],
      "react/jsx-props-no-spreading":"off",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ]
    }
  }
  